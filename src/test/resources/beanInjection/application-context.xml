<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	xmlns:p="http://www.springframework.org/schema/p">
	
	<bean id="boardDao" class="board.dao.BoardDao"/>
	
	<bean id="boardServiceSetter" class="board.service.impl.BoardServiceImpl">
		<property name="boardDao" ref="boardDao"/>
	</bean>
	
	<bean id="boardServiceConstructor" class="board.service.impl.BoardServiceImpl">
		<constructor-arg index="1" name="boardDao" ref="boardDao"/>
	</bean>
	
	<bean id="boardServiceConstructor2" class="board.service.impl.BoardServiceImpl">
		<constructor-arg name="boardDao" ref="boardDao"/>
		
	</bean>
	
</beans>	